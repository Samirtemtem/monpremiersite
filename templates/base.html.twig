<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		<link
		rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
			<div class="container-fluid">
				<a class="navbar-brand" href="#">Navbar</a>
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>

				<div class="collapse navbar-collapse" id="navbarColor02">
					<ul class="navbar-nav me-auto">
						<li class="nav-item">
							{% if app.request.attributes.get("_route")=="acceuil" %}
								<a class="nav-link active" href="/">
									<b>Home</b>
								</a>
							{% else %}
								<a class="nav-link" href="/">
									Home
								</a>
							{% endif %}
						</li>
					</li>
					<li class="nav-item">
						{% if app.request.attributes.get("_route")=="article" %}
							<a class="nav-link active" href="{{ path('article') }}">

								<b>Articles</b>
							</a>
						{% else %}
							<a class="nav-link" href="{{ path('article') }}">

								Articles
							</a>
						{% endif %}
					</li>
					<li class="nav-item">
						{% if app.request.attributes.get("_route")=="login" %}
							<a class="nav-link active" href="{{ path('login') }}">

								<b>Login</b>
							</a>
						{% else %}
							<a class="nav-link" href="{{ path('login') }}">Login</a>
						{% endif %}
					</li>

				</li>
			</ul>
		</div>
	</body>
</html></div></body></html></nav><span class="container"> {% block body %}{% endblock %}</span></body></html></body></html>
